<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="bottom|left"
    tools:context="com.example.joel.proyecto_incidencias.mapa_puntos_aprovados">

    <!-- TODO: Update blank fragment layout -->


    <com.google.android.gms.maps.MapView
        android:id="@+id/mapa_aprovados"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.getbase.floatingactionbutton.FloatingActionsMenu
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom|right"
        android:scrollY="@android:dimen/dialog_min_width_minor"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarSize="@android:dimen/dialog_min_width_minor"
        android:scrollbarTrackVertical="@drawable/camera"
        android:verticalScrollbarPosition="defaultPosition"
        tools:rotationY="90"
        tools:scrollbarSize="@dimen/activity_vertical_margin"

        >




        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_gaqq2y"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:fab_size="10dp"/>
        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_gssssa2y"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:fab_size="1"/>
        <com.getbase.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnaaa_ga2y"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:fab_size="1"/>

    </com.getbase.floatingactionbutton.FloatingActionsMenu>

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</FrameLayout>
